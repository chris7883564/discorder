	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/dist/index.js",
			"preLaunchTask": "build",
			"console": "integratedTerminal",
			"runtimeExecutable": "/usr/bin/env",
			"runtimeArgs": ["arch", "-x86_64", "node"]
		}
	]
}
